{
  "definitions": {
    "Rating": {
      "type": "string",
      "enum": [
        "mainstream",
        "general",
        "sensitive",
        "mature"
      ]
    },
    "ImageBase": {
      "type": "object",
      "properties": {
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "webp": {
          "type": "string"
        },
        "src": {
          "type": "string"
        },
        "thumbnailUrl": {
          "type": "string"
        },
        "rating": {
          "$ref": "#/definitions/Rating"
        },
        "aspectRatio": {
          "type": "number"
        },
        "href": {
          "type": "string"
        },
        "characters": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "tags",
        "src",
        "rating",
        "aspectRatio",
        "characters"
      ]
    },
    "ImageInformation": {
      "allOf": [
        {
          "$ref": "#/definitions/ImageBase"
        },
        {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "published": {
              "type": "string"
            },
            "artist": {
              "type": "string"
            }
          },
          "required": [
            "title",
            "published",
            "artist"
          ]
        }
      ]
    },
    "AltInformation": {
      "allOf": [
        {
          "$ref": "#/definitions/ImageBase"
        },
        {
          "type": "object",
          "properties": {
            "parent": {
              "type": "string"
            }
          },
          "required": [
            "parent"
          ]
        }
      ]
    }
  },
  "type": "array",
  "items": {
    "oneOf": [
      {
        "$ref": "#/definitions/ImageInformation"
      },
      {
        "$ref": "#/definitions/AltInformation"
      }
    ]
  }
}